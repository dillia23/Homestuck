{
  "manifest_version": 2,

  "name": "Homestuck EasyRead",
  "description": "This extension allows you to read Homestuck with more laziness.",
  "version": "1.0",
  "permissions": [
    "notifications",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["http://www.mspaintadventures.com/*"],
      "js": ["next.js"]
    }
  ]
}
